
type raw:
  def Expr(Expr.t)
  def LetVal(Pattern.t * Expr.t)
  def LetFun(list(Names.val_name * Expr.t * ref(list(Names.val_path))))
  def Open(Names.mod_path)
  def Type(list(TypeDef.t))
  def Exception(ExnDecl.t)
  def Class(Names.typeclass_name * TypeExpr.t * list(Names.val_name * TypeExpr.t * SchemeExpr.t))

type t:
  pos : Pos.t
  raw : raw

def at(pos, raw): {
  pos = pos;
  raw = raw;
}
