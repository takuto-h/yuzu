
def fold_left(init, list, func):
  List.fold_left(func, init, list)

def fold_right(list, init, func):
  List.fold_right(func, list, init)

def iteri(list, func):
  def loop(i, list):
    match list:
      case []:
        ()
      case x::xs:
        func(i, x)
        loop(i + 1, xs)
  loop(0, list)
