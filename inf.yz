
type t:
  dummy : unit
  
def create(): {
  dummy = ();
}

var int_type = Type.Con([], "int")
var string_type = Type.Con([], "string")
var char_type = Type.Con([], "char")

def infer_literal(lit):
  match lit:
    case Literal.Int(_):
      int_type
    case Literal.String(_):
      string_type
    case Literal.Char(_):
      char_type

def infer(inf, expr):
  match expr:
    case Expr.Con(lit):
      infer_literal(lit)
