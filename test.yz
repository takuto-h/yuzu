def fac_a(n):
  if(n == 0):
    1
  else:
    n * fac_a(n - 1)

var fac_a10 = fac_a(10)

def fac_b(n):
  var cond = (n == 0)
  var pred_n = (n - 1)
  if(cond):
    1
  else:
    n * fac_b(pred_n)

var fac_b10 = fac_b(10)

