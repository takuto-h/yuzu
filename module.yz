
type t:
  mods : list(Names.mod_name * t)
  asp : list(Names.val_name * Type.t)

def make(mods, asp): {
  mods = mods;
  asp = asp;
}

def find_asp(modl, mod_path, name):
  match mod_path:
    case []:
      List.assoc(name, modl.asp)
    case mod_name::mod_path:
      var modl = List.assoc(mod_name, modl.mods)
      find_asp(modl, mod_path, name)
